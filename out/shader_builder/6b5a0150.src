precision mediump float;uniform vec4 u_color;varying vec2 v_texCoord;uniform sampler2D u_texture;uniform sampler2D u_texture2;uniform float u_alphaOperator;uniform float u_alphaOffset;void main() {    float factor = u_alphaOffset + (u_alphaOperator * texture2D(u_texture2, v_texCoord).a);    gl_FragColor = texture2D(u_texture, v_texCoord) * (factor * u_color.a);}